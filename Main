
import java.awt.Color;
import java.awt.Dimension;
import javax.swing.JFrame;
import javax.swing.JPanel;


public class Main extends JFrame {
		public Main() {
			setBounds(100,200,500,500); //Setea la posicion del eje x e y de la ventana y ademas los tama√±os de la ventana
			setDefaultCloseOperation(EXIT_ON_CLOSE); //Permite salir de la ventana
			setTitle("DataLive"); //Establece el titulo
			
			//this.getContentPane().setBackground(Color.PINK);
			iniciarComponentes();
		}
		
		private void iniciarComponentes()
		{
			JPanel panel =new JPanel();
			
			panel.setBackground(Color.PINK);
			this.getContentPane().add(panel);
		}

}
